{
    "name": "crestic-urca/remotelabz-worker",
    "description": "VM-side of the RemoteLabz project.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "crestic-urca/network-bundle": "^1.0",
        "crestic-urca/remotelabz-message-bundle": "dev-dev",
        "doctrine/common": "^2",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "jms/serializer-bundle": "^3.1",
        "laminas/laminas-code": "^3.4",
        "laminas/laminas-eventmanager": "^3.2",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/amqp-messenger": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/error-handler": "5.1.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-kernel": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/process": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    },
    "require-dev": {
        "symfony/debug-bundle": "5.1.*",
        "symfony/maker-bundle": "^1.14",
        "symfony/stopwatch": "5.1.*",
        "symfony/web-profiler-bundle": "5.1.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/crestic-urca/network-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/crestic-urca/remotelabz-message-bundle.git"
        }
    ]
}
